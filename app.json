      "edgeToEdgeEnabled": true,
      "permissions": [
        "RECORD_AUDIO"
      ],
      "package": "com.prthkshk.u1rmobile"
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d5848762-4cc6-45ac-b7ba-c6109b2b33fe"
      }
    }
