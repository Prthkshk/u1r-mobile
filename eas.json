{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development",
        "EXPO_PUBLIC_API_BASE_URL": "https://api.u1rfoods.com"
      },
      "android": {
        "buildType": "apk"
      }
    },

    "preview": {
      "distribution": "internal",
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://api.u1rfoods.com"
      },
      "android": {
        "buildType": "apk"
      }
    },

    "production": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://api.u1rfoods.com"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },

  "submit": {
    "production": {}
  }
}
